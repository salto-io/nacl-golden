salesforce.Layout TimeSheet_Time_Sheet_Layout@bss {
  excludeButtons = [
    "Submit",
  ]
  layoutSections = [
    {
      customLabel = false
      detailHeading = true
      editHeading = true
      label = "Information"
      layoutColumns = [
        {
          layoutItems = [
            {
              behavior = "Required"
              field = salesforce.TimeSheet.field.StartDate
            },
            {
              behavior = "Edit"
              field = salesforce.TimeSheet.field.Status
            },
            {
              behavior = "Readonly"
              field = salesforce.TimeSheet.field.TimeSheetEntryCount
            },
          ]
        },
        {
          layoutItems = [
            {
              behavior = "Required"
              field = salesforce.TimeSheet.field.EndDate
            },
            {
              behavior = "Required"
              field = salesforce.TimeSheet.field.ServiceResourceId
            },
            {
              behavior = "Readonly"
              field = salesforce.TimeSheet.field.TotalDurationInHours
            },
          ]
        },
      ]
      style = "TwoColumnsLeftToRight"
    },
    {
      customLabel = false
      detailHeading = true
      editHeading = true
      label = "System Information"
      layoutColumns = [
        {
          layoutItems = [
            {
              behavior = "Readonly"
              field = salesforce.TimeSheet.field.CreatedById
            },
          ]
        },
        {
          layoutItems = [
            {
              behavior = "Readonly"
              field = salesforce.TimeSheet.field.LastModifiedById
            },
          ]
        },
      ]
      style = "TwoColumnsLeftToRight"
    },
    {
      customLabel = false
      detailHeading = false
      editHeading = false
      label = "Custom Links"
      style = "CustomLinks"
    },
  ]
  relatedLists = [
    {
      fields = [
        "TASK.SUBJECT",
        "TASK.WHO_NAME",
        "ACTIVITY.TASK",
        "TASK.DUE_DATE",
        "TASK.STATUS",
        "TASK.PRIORITY",
        "CORE.USERS.FULL_NAME",
      ]
      relatedList = "RelatedActivityList"
    },
    {
      fields = [
        "CreatedDate",
        "StepStatus",
        "OriginalActor",
        "Actor",
        "Comments",
      ]
      relatedList = "RelatedProcessHistoryList"
    },
    {
      relatedList = "RelatedFileList"
    },
    {
      relatedList = "RelatedEntityHistoryList"
    },
    {
      fields = [
        "Name",
        "Subject",
        "StartTime",
        "EndTime",
      ]
      relatedList = "TimeSheetEntries"
    },
  ]
  showEmailCheckbox = false
  showRunAssignmentRulesCheckbox = false
  showSubmitAndAttachButton = false
  fullName = "TimeSheet-Time Sheet Layout"
  _parent = [
    salesforce.TimeSheet,
  ]
  _alias = "TimeSheet-Time Sheet Layout"
}
